[Profiles/AAC]
    OutDirectory=~/Music/aac
    OutPattern=aac-%a/{%y - }%A/%n - %t
    Format=AAC
    Name=Aac
    Bitrate=128
    Quality=90
    UseQuality=false
    BitsPerSample=16
    SampleRate=44100
    CreateCue=false
    CueFileName = AAC %a %d - %A.cue
    PregapType = AddToFirst

[Profiles/FLAC]
    OutDirectory=~/Music/flac
    OutPattern=flac-%a/{%y - }%A/%n - %t
    Format=FLAC
    Name=Flac
    Compression=3
    ReplayGain=Track
    BitsPerSample=16
    SampleRate=44100
    CreateCue=true
    CueFileName = FLAC %a %d - %A.cue
    PregapType = Extract

[Profiles/MP3]
    OutDirectory=~/Music/mp3
    OutPattern=mp3-%a/{%y - }%A/%n - %t
    Format = MP3
    Name = Mp3
    Bitrate=320
    Preset=vbrStandard
    Quality=4
    ReplayGain=Disk
    BitsPerSample=16
    SampleRate=44100
    CreateCue=false
    CueFileName = MP3 %a %d - %A.cue
    PregapType = Disable

[Profiles/OGG]
